@using MVC_ONLINE_SATIS.Models.Sinifler
@model List < Kateqoriya>
<div style="box-shadow:0 0 5px black" class="card">
    <div style="background-color:#4b7bec" class="card-header">
        <h3 class="card-title">Məhsul kateqoriyaları</h3>
    </div>
    <!-- /.card-header -->
    <div class="card-body">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th style="width: 10px">ID</th>
                    <th>Kateqoriya</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var x in Model)
                {
                    <tr>
                        <td>@x.KateqoriyaID</td>
                        <td>@x.KateqoriyaAd</td>

                    </tr>
                }

            </tbody>
        </table>
    </div>
    <!-- /.card-body -->
</div>
